@model IEnumerable<PartStore.Core.StoreModels.Years>

@{
    ViewData["Title"] = "Years";
}

<h2>@ViewData["Title"]</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table table-bordered table-responsive table-hover table-striped">
    <thead>
        <tr>
            <th>Year</th>
            <th>Options</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.YearId</td>
                <td width="15%">
                    <a asp-action="Edit" asp-route-id="@item.YearId">Edit</a> |
                    <a asp-action="Delete" asp-route-id="@item.YearId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
